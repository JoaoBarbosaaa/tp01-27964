        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// Função para limpar texto genérico
function limparTexto(valor) {
  if (valor == null) return null;
  valor = valor.toString().trim();
  return valor.toUpperCase();
}

// Função para remover acentos e caracteres não-letra
function limparNome(valor) {
  if (valor == null) return null;
  var nome = valor.toString();
  // 1. Remover acentos (NFD)
  nome = java.text.Normalizer.normalize(nome, java.text.Normalizer.Form.NFD)
         .replaceAll("\\p{InCombiningDiacriticalMarks}+", "");
  // 2. Remover caracteres que não sejam letras ou espaços (opcional, dependendo do que quer manter)
  nome = nome.replaceAll("[^\\p{L}\\s]", "").trim();
  return nome.toUpperCase();
}
// Aplicação:
nome = limparNome(nome);
especialidade = limparNome(especialidade); // ou limparTexto(especialidade) se não for nome


// Função para normalizar NIF
function normalizarNif(valor) {
  if (valor == null) return null;
  valor = valor.toString().trim();
  // Remove tudo que não for número
  valor = valor.replace(/[^0-9]/g, '');
  return valor;
}

// Função para normalizar telefone
function normalizarTelefone(valor) {
  if (valor == null) return null;
  valor = valor.toString().trim();
  valor = valor.replace(/[^0-9]/g, '');
  return valor;
}

// Função para normalizar e formatar data
function normalizarData(valor) {
  if (valor == null || valor == "") return null;

  // Se for um objeto Date (como no teu XML)
  if (valor instanceof Date) {
    var ano = valor.getFullYear();
    var mes = ("0" + (valor.getMonth() + 1)).slice(-2);
    var dia = ("0" + valor.getDate()).slice(-2);
    return ano + "-" + mes + "-" + dia;
  }

  // Se vier como texto
  valor = valor.toString().trim();
  return valor;
}

// === Aplicação das funções aos campos ===

// id_paciente vem como texto → converter para número
id_paciente = parseInt(id_paciente);

// Campos de texto simples
nome = limparTexto(nome);
especialidade = limparTexto(especialidade);

// Campos numéricos/textuais específicos
nif = normalizarNif(nif);
telefone = normalizarTelefone(telefone);

// Campo de email (mantém minúsculas)
if (email != null) {
  email = email.toString().trim().toLowerCase();
}

// Campo de data
data_consulta = normalizarData(data_consulta);

